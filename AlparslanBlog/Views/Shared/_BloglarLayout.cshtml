
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="zxx">

<head>
	<title>Alparslan Blogger</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta charset="utf-8">
	<meta name="keywords" content="Weblog a Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template, 
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />

	<script>
		addEventListener("load", function () {
			setTimeout(hideURLbar, 0);
		}, false);

		function hideURLbar() {
			window.scrollTo(0, 1);
		}
	</script>
	<link href="~/BlogTema/css/bootstrap.css" rel='stylesheet' type='text/css' />
	<link rel="stylesheet" href="~/BlogTema/css/single.css">
	<link href="~/BlogTema/css/style.css" rel='stylesheet' type='text/css' />
	<link href="~/BlogTema/css/fontawesome-all.css" rel="stylesheet">
	<link href="//fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800"
	    rel="stylesheet">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body>
	<!--Header-->

	<header>
		<div class="top-bar_sub_w3layouts container-fluid">
			<div class="row">
				<div class="col-md-4 logo text-left">
					<a class="navbar-brand" href="/Yazarlar/Profilim">
						<i class="fab fa-linode"></i> Alparslan Blogger</a>
				</div>
				@await Html.PartialAsync("GirişPartial")
				<div class="col-md-4 log-icons text-right">

					<ul class="social_list1 mt-3">
						<li>
							<a href="https://twitter.com/AlparslanZombi" class="twitter2">
								<i class="fab fa-twitter"></i>

							</a>
						</li>
						<li>
							<a href="https://www.instagram.com/legolas.trr" class="dribble3 mx-2">
								<i class="fa fa-instagram"></i>
							</a>
						</li>
						<li>
							<a href="https://www.linkedin.com/in/alparslan-akbaş-29b732193/" class="pin">
								<i class="fab fa-linkedin"></i>
							</a>
						</li>
					</ul>
				</div>
			</div>
		</div>
		@await Html.PartialAsync("HeadPartial")
	</header>
	<!--//header-->
	<!--/banner-->
	<!--//banner-->
	@RenderBody()
	<!--/main-->
	
	<!--//main-->
	<!--footer-->
	@await Html.PartialAsync("FooterPartial")
	<!---->

	<!-- js -->
	<script src="~/BlogTema/js/jquery-2.2.3.min.js"></script>
	<!-- //js -->
	<!--/ start-smoth-scrolling -->
	<script src="~/BlogTema/js/move-top.js"></script>
	<script src="~/BlogTema/js/easing.js"></script>
	<script>
		jQuery(document).ready(function ($) {
			$(".scroll").click(function (event) {
				event.preventDefault();
				$('html,body').animate({
					scrollTop: $(this.hash).offset().top
				}, 900);
			});
		});
	</script>
	<!--// end-smoth-scrolling -->

	<script>
		$(document).ready(function () {
			/*
									var defaults = {
							  			containerID: 'toTop', // fading element id
										containerHoverID: 'toTopHover', // fading element hover id
										scrollSpeed: 1200,
										easingType: 'linear' 
							 		};
									*/

			$().UItoTop({
				easingType: 'easeOutQuart'
			});

		});
	</script>
	<a href="#home" class="scroll" id="toTop" style="display: block;">
		<span id="toTopHover" style="opacity: 1;"> </span>
	</a>

	<!-- //Custom-JavaScript-File-Links -->
	<script src="~/BlogTema/js/bootstrap.js"></script>


</body>

</html>
